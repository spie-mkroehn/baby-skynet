{
  "mcpServers": {
    "desktop-commander": {
      "command": "npx",
      "args": [
        "-y",
        "@wonderwhy-er/desktop-commander"
      ]
    },
    "baby-skynet": {
      "command": "node",
      "args": [
        "<path to>\\baby-skynet\\build\\index.js",
        "--db-path",
        "<path to>\\claude_memory.db",
        "--lancedb-path",
        "<path to>\\claude_lancedb",
	"--chromadb-path",
        "<path to>\\claude_chromadb",
	"--chroma-collection",
	"claude-main",
        "--brain-model",
        "claude-3-5-haiku-latest"
      ]
    }
  }
}